<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookku">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/iconapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity" />
        <activity android:name=".activity.SearchActivity" />
        <activity android:name=".activity.FilsafatdanPsikologi" />
        <activity android:name=".activity.Agama" />
        <activity android:name=".activity.Sosial" />
        <activity android:name=".activity.Bahasa" />
        <activity android:name=".activity.SainsdanMatematika" />
        <activity android:name=".activity.Teknologi" />
        <activity android:name=".activity.SenidanRekreasi" />
        <activity android:name=".activity.LiteraturdanSastra" />
        <activity android:name=".activity.SejarahdanGeografi" />
        <activity android:name=".activity.ViewBookActivity" />
        <activity android:name=".activity.BooksActivity" />
        <activity android:name=".activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>